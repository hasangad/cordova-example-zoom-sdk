<?xml version='1.0' encoding='utf-8'?>
<widget id="com.facetec.cordova.sample" version="1.0.0"  xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
<uses-sdk tools:overrideLibrary="acom.facetec.zoom.sdk"/>
         <preference name="android-minSdkVersion" value="15" />

    <name>ZoomCordova</name>
    <description>
        A sample Apache Cordova application using the ZoOm SDK plugin.
    </description>
    <author email="dev@cordova.apache.org" href="http://dev.zoomlogin.com">
        FaceTec, Inc
    </author>
                
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
    <preference name="AndroidLaunchMode" value="singleTop"/>
    <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.2.3" />
    <engine name="ios" spec="^4.4.0" />

    <plugin name="https://github.com/facetec/cordova-plugin-zoom-sdk" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.2" />
</widget>

